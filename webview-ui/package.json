    "@mui/lab": "5.0.0-alpha.89",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "checking-tool-rcl": "0.9.25-beta.12",
    "deep-equal": "1.0.1",
    "dompurify": "^3.0.9",
    "fuzzball": "^2.2.2",
    "gitea-react-toolkit": "2.4.1",
    "localforage": "^1.10.0",
    "marked": "^12.0.0",
    "mathjs": "^14.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "string-punctuation-tokenizer": "^2.2.0",
